{
  "name": "mindvault-semanticlinking",
  "version": "1.0.0",
  "description": "MindVault - Semantic Personal Notes with MongoDB Atlas Vector Search",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "mongodb",
    "vector-search",
    "semantic-search",
    "notes",
    "ai",
    "embeddings"
  ],
  "author": "",
  "license": "MIT"
}
